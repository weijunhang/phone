<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper" indicator-dots="true" indicator-active-color="#ff5777" autoplay="true" interval="2000" circular="true">
  <block wx:for="{{imgurl}}" wx:key="{{index}}">
    <swiper-item class="swiper-item" bindtap="navigate">
      <image src="{{item}}"></image>
    </swiper-item>
  </block>
</swiper>