/* pages/index/index.wxss */

.header{
  height: 400rpx;
  background:#333;
}

.hot{
  display: flex;
  text-align: center;
  padding: 20rpx 0;
}
.hot-tit{
  flex: 1;
  color: #fff;
  line-height: 40rpx;
  font-size: 20rpx;
}
.hot-text{
  display: flex;
  width: 80%;
}
.hot-text navigator{
  flex: 1;
  margin: 0 10rpx;
  background: #ff5777;
  border-radius: 20%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 20rpx;
}


/* <!-- 分类 --> */
.fen{
  width: 98%;
  margin: 10rpx 1%;
  display: flex;
  text-align: center;
  background: #eee;
  height: 80rpx;
  font-size: 25rpx;
}
.fen view{
  flex: 1;
  line-height: 60rpx;
}
.fen view text{
  padding: 10rpx 10rpx;
}
.fen view .active{
  border-bottom: 5rpx solid red;
  color: red;
}
.fenfixed{
  width: 100%;
  position: fixed;
  top: -10rpx;
  left: -10rpx;
  right: -10rpx;
}

/* 商品数据 */
.shopdata{
  display: flex;
  flex-wrap: wrap;
  width: 98%;
  margin: 0 1%;
  
}
.shopdata-box{
  flex: 1;
  width: 45%;
  text-align: center;
  padding: 0 10rpx;
}
.shopdata-img image{
  width: 100%;
  border-radius: 20rpx;
}
.shopdata-text view{
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

/* 加载更多 */
.loadmore{
  width: 750rpx;
  height: 200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.loading_img{
  width:50rpx;
  height:50rpx;
  margin-right: 20rpx;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/png;base64,) no-repeat;
 background-size: 100%;
}


/* 回到顶部 */
.backtop{
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  right: 20rpx;
  bottom: 40rpx;
}
.backtop image{
  width: 100%;
  height: 100%;
}
