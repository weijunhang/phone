/* pages/shopcar/shopcar.wxss */
.shops-box{
  width: 100%;
  height: 200rpx;
  border-bottom: 20rpx solid #eee;
  display: flex;
}
.shops-nullbox{
  width: 15%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.shops-null{
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
}
.no-backimg{
   border: 1px solid #333;
}
.backimg{
  background:transparent url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.shops-img{
  display: flex;
  align-items: center;
}
.shops-img image{
  width: 180rpx;
  height: 180rpx;
  border-radius: 20rpx;
}
.shops-con{
  height: 200rpx;
  padding-left: 25rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.shops-text{
  font-size: 40rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.shops-pic{
  width:400rpx;
  font-size:40rpx;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-around;
}

/* 组件的样式 */
.tan-suan{
  border: 1rpx solid #666;
  margin:0 15rpx;
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
}
.saun-two{
  border-left: 1rpx solid #666;
  border-right: 1rpx solid #666;
}
/* .saun-two::after{
  content: '';
